{
  "cleanUrls": true,
  "public": true,
  "functions": {
    "api/signup-proxy.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/signup-proxy",
      "destination": "/api/signup-proxy"
    }
  ]
}